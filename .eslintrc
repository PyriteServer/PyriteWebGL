// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "rules": {
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ],
    "linebreak-style": [
      "off"
    ],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": [
          "**/test/*.js"
        ]
      }
    ]
  }
}